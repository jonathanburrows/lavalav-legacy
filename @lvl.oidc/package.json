{
  "name": "@lvl/oidc",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "generate-models": "node node_modules/@lvl/core/dotnet-runner.js ../@lvl.typescript-generator --assembly-path=../lvl.Oidc.AuthorizationServer/bin/Debug/net461/lvl.Oidc.AuthorizationServer.exe --output-bin=./src/models lvl.Ontology=@lvl/core",
    "seed-oidc": "node node_modules/@lvl/core/dotnet-runner.js ../@lvl.oidc-seeder --seed-test-data --connection-string \"Server=.;Database=LvlTest;Trusted_Connection=True;\"",
    "init.dev": "npm link ../@lvl.core",

    "build.dev": "ng build --progress=false --output-path=./dist/dev",
    "serve.dev": "ng serve --progress=false --output-path=./dist/dev --open",

    "build.prod": "ng build --prod --aot --output-path=./dist/prod",
    "serve.prod": "ng serve --prod --aot --output-path=./dist/prod --open",

    "lint": "ng lint",
    "test": "ng test",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "4.0.2",
    "@angular/common": "4.0.2",
    "@angular/compiler": "4.0.2",
    "@angular/core": "4.0.2",
    "@angular/forms": "4.0.2",
    "@angular/http": "4.0.2",
    "@angular/material": "2.0.0-beta.3",
    "@angular/platform-browser": "4.0.2",
    "@angular/platform-browser-dynamic": "4.0.2",
    "@angular/router": "4.0.2",
    "@lvl/core": "file:../@lvl.core",
    "core-js": "2.4.1",
    "rxjs": "5.3.0",
    "zone.js": "0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0",
    "@angular/compiler-cli": "4.0.2",
    "@types/jasmine": "2.5.38",
    "@types/node": "7.0.12",
    "codelyzer": "2.0.0",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "3.2.0",
    "karma": "1.4.1",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-coverage-istanbul-reporter": "0.2.0",
    "npm-run-all": "4.0.2",
    "protractor": "5.1.0",
    "ts-node": "2.0.0",
    "tslint": "4.5.0",
    "typescript": "2.2.0"
  }
}
